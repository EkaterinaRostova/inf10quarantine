A.
var a,b,f,d,c: integer;
var e: real;
begin
read(a,b,c);
f:=a+b+c;
d:=a*b*c;
e:=f/3;
writeln(a,'+',b,'+',c,'=',f);
writeln(a,'*',b,'*',c,'=',d);
writeln('(',a,'+',b,'+',c,')/3=',e:3:3);
end.

B. С++
#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;
int main()
{
double r, s, c;
cin >> r;
s = M_PI*r*r;
c = 2*M_PI*r;
cout<<fixed<<setprecision(3)<< s << "\n" << c << "\n";
return 0;
}

C. C++
#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;
int main()
{
double a,b=10;
cin >> a;
cout<<fixed<<setprecision(3)<<pow(a,b);
return 0;
}

D. C++
#include <iostream>
using namespace std;
int main()
{
    int a,b,c,d;
    cin>>a;
    b=a/100;
    c=(a/10)%10;
    d=a%10;
    cout<<b<<' '<<c<<' '<<d;
    return 0;
}

