F. C++
#include <iostream>
using namespace std;
#include <math.h>
#include<iomanip>
int main()
{
    double a,b;
    cin>>a>>b;
    cout<<fixed<<setprecision(3)<<pow(a,b);
    
    return 0;
}

G. C++
#include <iostream>
#include <stdlib.h>
#include <time.h>
using namespace std;
int main ()
{
    int a,b,random_number;
    cin>>a>>b;
     srand(time(NULL));
     for (int i = 0; i < 5; i++) {
    random_number=a+rand() %(b - a);
    cout<<random_number<<" ";
     }
    return 0;
}

H. C++(частичное решение)
#include <iostream>
#include <stdlib.h>
#include <time.h>
using namespace std;
int main ()
{
    float a,b,random_number;
    cin>>a>>b;
     srand(time(NULL));
     for (int i = 0; i < 5; i++) {
    random_number=a+rand() % int ( b-a); 
    cout<<random_number<<" ";
     }
    return 0;
}
