A. C++
#include <iostream>
using namespace std;

int main()
{
    int a, b, c = 0;
cin >> a >> b;
if (abs(a) > abs(b))
swap (a, b);
for (int i = 0; i < abs(a); i++)
{
if ((a >= 0 && b >= 0) || (a < 0 && b < 0))
c += abs(b);
else
c -= abs(b);
}
cout << c;
    return 0;
}

B. C++
#include <iostream>
using namespace std;

int main()
{
    int a,i=0,b=0;
    cin>>a;
    while(i<a){
        i++;
        b=b+i;
    }
    cout<<b;
    return 0;
}

C. C++
#include <iostream>
using namespace std;

int main()
{
    int a,i=0,b=0;
    cin>>a;
    while(i<a){
        i++;
        b=b+2;
        cout<<b<<' ';
    }
    
    return 0;
}

D. C++
#include <iostream>
using namespace std;

int main()
{
    int a,b,i;
    cin>>a>>b;
    i=a;
    while(i<=b){
        cout<<i<<'*'<<i<<'='<<i*i<<'\n';
        i++;
    }
    
    return 0;
}

E. C++
#include <iostream>
using namespace std;

int main()
{
    int a,b,i,d=0;
    cin>>a>>b;
    i=a;
    while(i<=b){
        d=d+(i*i);
        i++;
    }
    cout<<d;
    return 0;
}

F. C++
#include <iostream>
using namespace std;

int main()
{
    int a,b,i=0,d=0,N;
    cin>>a>>b>>N;
    while(i<N){
        cout<<rand() % (b - a + 1) + a<<" ";
        i++;
    }
    return 0;
}

G. C++ 
#include <stdio.h>
#include <iostream>
using namespace std;
int main()
{
 long a,b,c,d,i;
 bool z=false;
 cin>>a;
 cin>>b;
 cin>>c;
 cin>>d;
 for (i=10000; i<=99999; i++)
 {
   if (i % a==b && i % c==d) {z=true; cout<<i<<" ";}
 }
 if (z==false) {cout<<"-1";} // Выводится если нет чисел
}

