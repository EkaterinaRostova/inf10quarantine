J. PascalABC
var a,b,i,c: integer;
    f:boolean;{f:integer;}
begin
  read(a,b);
  c:=10;
  f:=false;{f:=0;}
    for i:=a to b do
      begin
      while c<=i do c:=c*10;
        begin
        if i*i mod c=i then
                         begin
                          write(i,' ');
                          f:=true{f:=1}
                         end 
                        
                       
       end;
      end;
     if not f{f=0} then write('-1') 
end.

K. C++
#include <iostream>
using namespace std;

int main()
{
    int a,b,i=0;
    cin>>a;
    while(a>0){
        b=a%10;
        if(b%2==0){
            i++;
        }
        a=a/10;
    }cout<<i;
    return 0;
}

L. C++
#include <iostream>
using namespace std;

int main()
{
    int a,b=0,i=0;
    cin>>a;
    while(a>0){
        b=b+a%10;
        
        a=a/10;
    }cout<<b;
    return 0;
}

M. -
N. PascalABC
var n,a,k:integer;
begin
readln(n);
a:=n mod 10;
n:=n div 10;
k:=0;
while (n>0)and(k=0) do
if n mod 10<>a then k:=1
else n:=n div 10;
if k=0 then write('YES')
else write('NO');
end.

