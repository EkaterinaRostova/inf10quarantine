O. C++
#include <cmath>
#include <iostream>
using namespace std;

int main()
{
    int N,i,a=1,b=0,c;
    cin>>N;
    for(i=0;i<N;i++){
        c=b+a;
        a=b;
        b=c;
        cout<<c<<' ';
    }
    return 0;
}
